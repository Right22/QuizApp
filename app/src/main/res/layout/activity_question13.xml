<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#779ECC"
    tools:context="com.example.ekasilabalexcdtb.quizapp.MainActivity">



    <RelativeLayout
        android:layout_width="200dp"
        android:layout_height="90dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/relativeLayout2">
        <ImageView
            android:layout_width="200dp"
            android:layout_height="90dp"
            android:src="@drawable/androidpictureaaaa"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

    </RelativeLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" if your XML layout is saved as main_layout.xml, you would not load it for your Activity like so."
        android:textColor="#020205"
        android:textSize="18dp"
        android:textStyle="italic"
        android:id="@+id/textView"
        android:layout_marginTop="25dp"
        android:layout_below="@+id/relativeLayout2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/imageButton1111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="True"
        android:onClick="Button1111"
        android:layout_marginBottom="14dp"
        android:layout_above="@+id/imageButton2222"
        android:layout_alignLeft="@+id/imageButton2222"
        android:layout_alignStart="@+id/imageButton2222" />
    <Button
        android:id="@+id/imageButton2222"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="False"
        android:onClick="Button2222"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="130dp" />


</RelativeLayout>
