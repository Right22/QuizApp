<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#779ECC"
    tools:context="com.example.ekasilabalexcdtb.quizapp.MainActivity">



    <RelativeLayout
        android:layout_width="400dp"
        android:layout_height="90dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/relativeLayout2">
        <ImageView
            android:layout_width="400dp"
            android:layout_height="90dp"
            android:src="@drawable/androidpictureaaaa"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

    </RelativeLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Java supports the following fundamental concepts: Polymorphism, Inheritance, Encapsulation, Abstraction, Classes, Objects, Instance, Method, and Message Parsing."
        android:textColor="#f402fa"
        android:textSize="18dp"
        android:textStyle="italic"
        android:id="@+id/textView"
        android:layout_marginTop="41dp"
        android:layout_below="@+id/relativeLayout2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />



    <CheckBox
        android:id="@+id/imageButton3333"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="True"
        android:onClick="Button3333"
        android:layout_marginTop="53dp"
        android:layout_below="@+id/textView"
        android:layout_alignLeft="@+id/imageButton4444"
        android:layout_alignStart="@+id/imageButton4444" />
    <CheckBox
        android:id="@+id/imageButton4444"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="False"
        android:onClick="Button4444"
        android:layout_marginTop="17dp"
        android:layout_below="@+id/imageButton3333"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
